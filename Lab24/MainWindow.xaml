<Window x:Class="Lab24.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab24"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <ComboBox x:Name="Year" HorizontalAlignment="Left" Margin="50,60,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="ComboBox_SelectionChanged" Loaded="Year_Loaded"/>
        <ComboBox x:Name="Mounth" HorizontalAlignment="Left" Margin="220,60,0,0" VerticalAlignment="Top" Width="120" RenderTransformOrigin="0.479,0.462" IsEnabled="False" Loaded="Mounth_Loaded" SelectionChanged="Mounth_SelectionChanged"/>
        <ComboBox x:Name="Day" HorizontalAlignment="Left" Margin="374,60,0,0" VerticalAlignment="Top" Width="120" IsEnabled="False" Loaded="Day_Loaded"/>
    </Grid>
</Window>
